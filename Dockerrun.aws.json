{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"crimes-client",
            "image":"sunpausiddharth/crimes-new-dashboard-copy-crimes-client",
            "hostname":"crimes-client",
            "essential":"false",
            "memory": 128
        },
        {
            "name":"backend",
            "image":"sunpausiddharth/crimes-new-dashboard-copy-backend",
            "hostname":"backend",
            "portMappings":[
                {
                    "hostPort":8004,
                    "containerPort":8004
                }
            ],
            "essential":"false",
            "memory": 128
        },
        {
            "name":"crime_data_explorer",
            "image":"sunpausiddharth/crimes-new-dashboard-copy-crime_data_explorer",
            "hostname":"crime-data-explorer",
            "essential":"false",
            "portMappings":[
                {
                    "hostPort":8007,
                    "containerPort":8007
                }
            ],
            "memory": 128
        },
        {
            "name":"elasticsearch",
            "image":"docker.elastic.co/elasticsearch/elasticsearch:7.5.1",
            "hostname":"elasticsearch",
            "essential":"false",
            "portMappings":[
                {
                    "hostPort":9200,
                    "containerPort":9200
                }
            ],
            "memory": 128
        },
        {
            "name":"redis",
            "image":"redis:latest",
            "hostname":"redis",
            "essential":"false",
            "memory": 128
        },
        {
            "name":"database",
            "image":"mongo",
            "hostname":"database",
            "essential":"false",
            "memory": 128
        },
        {
            "name":"nginx",
            "image":"sunpausiddharth/crimes-new-dashboard-copy-nginx",
            "hostname":"nginx",
            "essential":"true",
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["crimes-client","backend","crime_data_explorer"],
            "memory": 128
        }
    ]
}